#!/usr/bin/env node
const postcss = require('postcss')
const stylefmt = require('stylefmt')
const generate = require('../lib/postcss-plugin')

postcss([generate, stylefmt])
  .process(`/* THIS IS GENERATED. DO NOT EDIT IT! */`, {
    from: undefined
  })
  .then(({css}) => {
    process.stdout.write(css)
  })
