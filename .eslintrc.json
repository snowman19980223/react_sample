{
  "extends": [
    "plugin:github/es6",
    "plugin:github/react",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "import/no-namespace": 0,
    "no-shadow": 0,
    "no-unused-vars": ["error", {
      "ignoreRestSiblings": true
    }],
    "eslint-comments/no-use": 0,
    "jsx-a11y/label-has-for": [
      2,
      {
        "components": []
      }
    ]
  }
}
